  }
  gettimefun(){
    setTimeout(() => {
      if (this.seletecdPg > 0) {
        var innPgCanvas = this.arrPageCanvas[this.seletecdPg - 1];
        innPgCanvas.renderAll();
      }
    }, 100);
  }
  btnAddaccinitSignClick(signaturepopups) {
    this.selectedsignature_name='';
this.selectedsignature_id='';
