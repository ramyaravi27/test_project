  setCanvasImage() {
    let self = this;
    if (this.props.canvasImage) {
      this.tempobj.setBackgroundColor({ source: this.props.canvasImage, repeat: 'repeat' }, function () {
        // self.props.canvasFill = '';
        self.canvas.renderAll();
      });
    }
  }
 
