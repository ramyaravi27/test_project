  this.canvasarray=[];
    this.userService.getAllSlidePages(designid)
      .subscribe(data => {
        // console.log("canvasarray",data)
        for(var i=0;i<data.imagefile.length;i++){
      //  this.loadpagefun1(data.Templatejson[i][0]);
     this.canvasarray = data.imagefile;
        }
      })
}
