  if(this.dashboardmenu===false){
    this.dashboardmenu=true;
  }else{
    this.dashboardmenu=false;
  }
}
showpdfmenus(){
  this.pdfmenus=!this.pdfmenus;
}
showdropdownmenu(){
  if(this.dropdownmenus===false){
    this.dropdownmenus=true;
  }else{
    this.dropdownmenus=false;
  }
  this.toggleClose();
}
